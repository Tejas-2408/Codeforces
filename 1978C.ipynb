    "def sol():\n",
    "    n,k = map(int,input().split())\n",
    "    print(n,k)\n",
    "    x = k/2\n",
    "    if k%2!=0:\n",
    "        print(\"No\")\n",
    "    elif x<=n-1:\n",
    "        print(\"Yes\")\n",
    "    else:\n",
    "        print(\"Yes\")"
